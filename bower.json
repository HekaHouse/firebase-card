{
  "name": "firebase-card",
  "version": "1.0.1",
  "authors": [
    "Aron Price <aron@heka.house>"
  ],
  "description": "wraps the content of a firebase-document into a paper-card and creates firebase-input elements for each value in the document.",
  "keywords": [
    "web-component",
    "polymer",
    "firebase",
    "paper-card",
    "paper-input"
  ],
  "main": "firebase-card.html",
  "license": "https://heka-house-polymer-demos.firebaseapp.com/LICENSE",
  "homepage": "https://github.com/HekaHouse/firebase-card/",
  "ignore": [
    "/.*",
    "/test/"
  ],
  "dependencies": {
    "polymer": "Polymer/polymer#^1.2.0",
    "firebase-element": "GoogleWebComponents/firebase-element#^1.0.12",
    "paper-card": "PolymerElements/paper-card#^1.1.1",
    "iron-collapse": "PolymerElements/iron-collapse#^1.0.8",
    "firebase-input": "HekaHouse/firebase-input"
  },
  "devDependencies": {
    "iron-component-page": "PolymerElements/iron-component-page#^1.0.0",
    "web-component-tester": "^4.0.0"
  }
}
